{
  "name": "jediyozh.com",
  "version": "1.0.0",
  "description": "Personal site of Eli Levit",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/jediyozh/jediyozh.com.git"
  },
  "license": "ISC",
  "author": "Eli Levit",
  "scripts": {
    "start": "eleventy --serve --quiet",
    "build:eleventy": "eleventy --quiet",
    "build:parcel": "cp -a _site/other dist/ && parcel build  _site/en/index.html _site/en/about/index.html _site/ru/index.html _site/ru/about/index.html _site/ru/blog/index.html _site/en/blog/index.html _site/en/blog/index.html _site/ru/blog/index.html",
    "build": "npm run remove-builds && npm run build:eleventy && npm run build:parcel",
    "test": "echo \"Error: no test specified\" && exit 1",
    "remove-builds": "rm -rf dist && rm -rf _site && rm -rf .cache",
    "push-both": "git push origin && git push github"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.9.0",
    "parcel-bundler": "^1.12.4"
  }
}
